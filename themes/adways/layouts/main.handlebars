<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>{{htmlTitle}}</title>
    <link rel="stylesheet" href="{{yuiGridsUrl}}">
    <link rel="stylesheet" href="{{projectAssets}}/vendor/prettify/prettify-min.css">
    <link rel="stylesheet" href="{{projectAssets}}/css/main.css" id="site_styles">
    <script src="{{yuiSeedUrl}}"></script>
    <script src="https://cdn.rawgit.com/google/code-prettify/master/loader/run_prettify.js?lang=css&amp;skin=sunburst"></script>
</head>
<body class="yui3-skin-sam">

<div id="doc">
    <div id="hd" class="yui3-g header">
        <div class="yui3-u-3-4">
            <h2><a href="{{projectRoot}}../../index.html">
                <img src="{{projectAssets}}/css/logo.png" width="150" height="44"></a>
                / <a href="{{projectRoot}}index.html">Overview</a> / {{title}}</h2>
        </div>
        <div class="yui3-u-1-4 version">
            <em>API Docs for: {{projectVersion}}</em>
        </div>
        <div class="beta-logo"></div>
    </div>
    <div class="yui3-g main-content">
        <div id="sidebar" class="yui3-u">
            {{>sidebar}}
            <div id="tools" class="sidebox">
                <div class="hd">
                    <h2 class="no-toc">Tools</h2>
                </div>
                <div class="bd">
                    <ul>
                        <li><a href="/doc/interactive/console/index.html">Adways "Experience" Test Console</a></li>
                    </ul>
                </div>
            </div>
            <div id="samples" class="sidebox">
                <div class="hd">
                    <h2 class="no-toc">Samples</h2>
                </div>
                <div class="bd">
                    <ul>
                        <li><a href="/doc/interactive/samples/sample1.html">#1 Standard embed share code</a></li>
                        <li><a href="/doc/interactive/samples/sample2.html">#2 Instantiation without the createExperience factory</a></li>
                        <li><a href="/doc/interactive/samples/sample3.html">#3 Interactivity on top of HTML5 video tag</a></li>
                        <li><a href="/doc/interactive/samples/sample4.html">#4 Interactivity on top of VideoJS4</a></li>
                        <li><a href="/doc/interactive/samples/sample5.html">#5 Interactivity on top of JWPlayer6</a></li>
                        <li><a href="/doc/interactive/samples/sample6.html">#6 Interactivity on top of YouTube</a></li>
                        <li><a href="/doc/interactive/samples/sample7.html">#7 Interactivity on top of Vimeo</a></li>
                        <li><a href="/doc/interactive/samples/sample8.html">#8 Interactivity on top of Dailymotion</a></li>
                        <li><a href="/doc/interactive/samples/sample9.html">#9 Interactivity on top of YouTube Live stream</a></li>
                    </ul>
                </div>
            </div>
        </div>

        <div id="main" class="yui3-u">
            <div class="content">{{>layout_content}}</div>
        </div>
    </div>
    <div class="footer">
        <div class="container">
            <p>&copy; 2016 <a href="http://www.adways.com" target="_blank">Adways</a>. All rights reserved.</p>
        </div>
    </div>
</div>
<script src="{{projectAssets}}/vendor/prettify/prettify-min.js"></script>
<script>prettyPrint();</script>
<script src="{{projectAssets}}/js/yui-prettify.js"></script>
<script src="{{projectAssets}}/js/tabs.js"></script>
<script type="text/javascript">
////////////////////////////
// script toggle sidebox
///////////////////////////
var toggleSideBox = function(container, button){
    if(container.offsetHeight > 40){
        container.style.height = "34px";
        button.className = "no-toc collapsed";
    }else{
        container.style.height = "100%";
        button.className = "no-toc";
    }
};
var toggleButtons = new Array();
var classBoxes = document.getElementsByClassName("sidebox");
if(classBoxes.length > 0){
    for(var i = 0; i < classBoxes.length; i++){
        toggleButtons[i] = classBoxes[i].getElementsByClassName("no-toc")[0];
        if(typeof toggleButtons[i] != "undefined"){
            if(i > 0){
                toggleSideBox(classBoxes[i],toggleButtons[i]);
            }
            (function(container, button){
                toggleButtons[i].onclick = function(){
                    toggleSideBox(container,button);
                };
            })(classBoxes[i], toggleButtons[i]);
        }
    }
}
////////////////////////////
// end script toggle sidebox
///////////////////////////
</script>
</body>
</html>
