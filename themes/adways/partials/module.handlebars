<h2>{{moduleName}}</h2>
<div class="intro">{{{moduleDescription}}}</div>

<div class="yui3-g">
    <div class="yui3-u-1-2">
        {{#if moduleClasses}}
            <p>This module has the following classes:</p>
            <ul>
                {{#moduleClasses}}
                    <li><a href="{{../projectRoot}}classes/{{name}}.html">{{displayName}}</a></li>
                {{/moduleClasses}}
            </ul>
        {{/if}}
    </div>
    <div class="yui3-u-1-2">
        {{#if moduleElements}}
            <p>This module has the following elements:</p>
            <ul>
                {{#moduleElements}}
                    <li><a href="{{../projectRoot}}elements/{{name}}.html">{{displayName}}</a></li>
                {{/moduleElements}}
            </ul>
        {{/if}}
    </div>
    <div class="yui3-u-1-2">
        {{#if moduleInterfaces}}
            <p>This module has the following interfaces:</p>
            <ul>
                {{#moduleInterfaces}}
                    <li><a href="{{../projectRoot}}interfaces/{{name}}.html">{{displayName}}</a></li>
                {{/moduleInterfaces}}
            </ul>
        {{/if}}
    </div>
    <div class="yui3-u-1-2">
        {{#if subModules}}
            <p>This module has the following modules:</p>
            <ul>
                {{#subModules}}
                    <li id="{{name}}"><a href="{{../projectRoot}}modules/{{name}}.html">{{displayName}}</a>
                        <p>{{{description}}}</p>
                    </li>
                {{/subModules}}
            </ul>
        {{/if}}
    </div>
</div>
<br>
<br>
