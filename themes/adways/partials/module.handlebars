
<h2>{{moduleName}}</h2>
<div class="intro">{{{moduleDescription}}}</div>

<div class="yui3-g">
    <div class="yui3-u-1-2">
        {{#if moduleClasses}}
        <p>This namespace has the following classes:</p>
        <ul>
        {{#moduleClasses}}
            <li><a href="{{../projectRoot}}classes/{{name}}.html">{{displayName}}</a></li>
        {{/moduleClasses}}
        </ul>
        {{/if}}
    </div>
    <div class="yui3-u-1-2">
        {{#if moduleElements}}
        <p>This namespace has the following elements:</p>
        <ul>
        {{#moduleElements}}
            <li><a href="{{../projectRoot}}elements/{{name}}.html">{{displayName}}</a></li>
        {{/moduleElements}}
        </ul>
        {{/if}}
    </div>
    <div class="yui3-u-1-2">
        {{#if subModules}}
        <p>This namespace has the following namespaces:</p>
        <ul>
        {{#subModules}}
            <li id="{{name}}"><a href="{{../projectRoot}}modules/{{name}}.html">{{displayName}}</a><p>{{{description}}}</p></li>
        {{/subModules}}
        </ul>
        {{/if}}
    </div>
</div>
<br><br>
