<h4>Interface  {{moduleName}}{{#if nestedOf}} nested of {{#crossLink nestedOf}}{{{nestedOf}}}{{/crossLink}}{{/if}}</h4>
{{#if uses}}
    Uses:
    {{#each uses}}
        <a href="{{this}}.html">{{this}}</a>
    {{/each}}
    <br>
{{/if}}
{{#if extension_for}}
    Extension For:
    {{#each extension_for}}
        <a href="{{this}}.html">{{this}}</a>
    {{/each}}
    <br>
{{/if}}
{{#if extends}}
    Extends:
    {{#crossLink extends}}{{/crossLink}}
    <br>
{{/if}}

{{#fires}}
    <div class="fires">
        <strong>Fires:</strong>
        {{#if type}}
            <span class="type">&lt;{{#crossLink type}}{{/crossLink}}&gt;</span>:
        {{/if}}
        {{#if kind}}
            of kind <span class="type">{{#crossLink kind}}{{/crossLink}}</span>:
        {{/if}}
        {{#if description}}
            {{{description}}}
        {{/if}}
    </div>
{{/fires}}

<div class="intro">{{{interfaceDescription}}}</div>

{{#if nested}}
    <p>Nested classes:
        {{#nested}}
            {{#crossLink this}}
                {{#basicName this}}{{/basicName}}
            {{/crossLink}},
        {{/nested}}
    </p>
{{/if}}

{{#if is_constructor}}
    {{#is_constructor}}
        {{>method}}
    {{/is_constructor}}
{{/if}}

<div id="classdocs">
    <ul>
        {{#if methods}}
            <li><a href="#methods">Methods</a></li>
        {{/if}}
        {{#if properties}}
            <li><a href="#props">Properties</a></li>
        {{/if}}
        {{#if attrs}}
            <li><a href="#attrs">Attributes</a></li>
        {{/if}}
        {{#if events}}
            <li><a href="#events">Events</a></li>
        {{/if}}
    </ul>
    <div>
        {{#if methods}}
            <div id="methods">
                {{#methods}}
                    {{>method}}
                {{/methods}}
            </div>
        {{/if}}
        {{#if properties}}
            <div id="props">
                {{#properties}}
                    {{>props}}
                {{/properties}}
            </div>
        {{/if}}
        {{#if attrs}}
            <div id="attrs">
                {{#attrs}}
                    {{>attrs}}
                {{/attrs}}
            </div>
        {{/if}}
        {{#if events}}
            <div id="events">
                {{#events}}
                    {{>events}}
                {{/events}}
            </div>
        {{/if}}
    </div>
</div>
